git init  // Para Iniciar Git
commint -m // Para Confirmar los Cambios 
Cat "Nombre del archivo" // para leer archivo
git add . //para agregar todo 
git show "Nombre del archivo" // Para Mostrar Cambios Realizados
git log "Nombre del Archivo" // para ver historial de modificaciones
git diff + clave + clave // Se colocan las claves con que se van a comparar
git reset + clave +  hard or soft // para volver a tras sin retorno
git checkout + clave + nombre de archivo // sierve para volver atras con retorno
git branch + "Nombre de la nueva rama"
git merge  + Nombre dela rama con la que vas a unir
git remote origin + url // Crea el enlace entre git and gighob 
git push origin (Nombre de la rama) // envia la rama principal al gighob
git pull origin master --allow-unrelated-histories
git config --global user.email "Correo que se usara" // asignar o cambiar Correo
ssh-keygen -t rsa -b 4096 -C "joregesosa@gmail.com" // Crear llave de cifrado
ssh-add /home/jorge/.ssh/id_rsa  // Agregar 
eval $(ssh-agent -s)
git remote set-url origin + nueva Url // Cambiar url del proyecto
